{
  "name": "vesiliikenne-maas",
  "version": "1.0.0",
  "repository": "git@github.com:City-of-Helsinki/vesiliikenne-maas.git",
  "license": "MIT",
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "dev": "next",
    "start": "next start -p $PORT",
    "clean": "rm -rf build",
    "build": "next build",
    "build-scripts": "tsc -p ./scripts/tsconfig.json",
    "create-api-key": "yarn clean && yarn run build-scripts && node build/scripts/create-api-key.js",
    "fetch-jwt-ticket": "yarn clean && yarn run build-scripts && node build/scripts/fetch-ticket-from-api.js",
    "doc": "swagger-jsdoc -o public/specs/routes.json -d public/specs/swaggerDef.js",
    "migrate": "node-pg-migrate",
    "run-migrations-dev": "DATABASE_URL=postgresql://postgres:hki-vesiliikenne@localhost:5432/postgres node-pg-migrate",
    "test": "DATABASE_URL=postgresql://postgres:hki-vesiliikenne@localhost:5432/postgres jest",
    "heroku-postbuild": "next build && node-pg-migrate up",
    "ci-test": "DATABASE_URL=postgresql://postgres:postgres@localhost:5432/postgres jest",
    "ci-migrate-up": "DATABASE_URL=postgresql://postgres:postgres@localhost:5432/postgres node-pg-migrate up"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "fp-ts": "^2.6.7",
    "io-ts": "^2.2.7",
    "jsonwebtoken": "^8.5.1",
    "leaflet": "^1.6.0",
    "leaflet-polylinedecorator": "^1.6.0",
    "moment-timezone": "^0.5.31",
    "next": "^9.4.4",
    "node-pg-migrate": "^5.2.0",
    "pg": "^8.2.1",
    "qrcode": "^1.4.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-leaflet": "^2.7.0",
    "uuidv4": "^6.1.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@types/bcryptjs": "^2.4.2",
    "@types/jest": "^25.2.3",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.0.14",
    "@types/pg": "^7.14.3",
    "@types/qrcode": "^1.3.4",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-leaflet": "^2.5.2",
    "@types/test-listen": "^1.1.0",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "babel-jest": "^26.1.0",
    "eslint": "^7.1.0",
    "jest": "^26.0.1",
    "swagger-jsdoc": "^4.0.0",
    "test-listen": "^1.1.0",
    "ts-jest": "^26.1.0",
    "typescript": "^3.9.5"
  }
}
